{
  "/dev/sdb": {
    "partitions": [
      {
        "boot": true,
        "encrypted": false,
        "filesystem": {
          "format": "fat32"
        },
        "mountpoint": "/boot",
        "size": "512MB",
        "start": "1MB",
        "type": "primary",
        "wipe": true
      },
      {
        "btrfs": {
          "subvolumes": {
            "@": "/",
            "@log": "/var/log",
            "@pkgs": "/var/cache/pacman/pkg",
            "@.snapshots": "/.snapshots",
            "@tmp": "/tmp"
          }
        },
        "encrypted": false,
        "filesystem": {
          "format": "btrfs",
          "mount_options": [
            "compress=zstd"
          ]
        },
        "wipe": true,
        "size": "100%",
        "start": "513MB",
        "type": "primary"
      }
    ],
    "wipe": true
  },
  "/dev/sda": {
    "partitions": [
      {
        "btrfs": {
          "subvolumes": {
            "@home": "/home"
          }
        },
        "encrypted": false,
        "filesystem": {
          "format": "btrfs",
          "mount_options": [
            "compress=zstd"
          ]
        },
        "size": "100%",
        "start": "1MB",
        "type": "primary",
        "wipe": true
      }
    ],
    "wipe": true
  }
}