{
  "/dev/sdb": {
    "partitions": [
      {
        "boot": true,
        "encrypted": false,
        "filesystem": {
          "format": "fat32"
        },
        "wipe": true,
        "mountpoint": "/boot",
        "size": "512MB",
        "start": "1MB",
        "type": "primary"
      },
      {
        "btrfs": {
          "subvolumes": {
            "@log": "/var/log",
            "@pkgs": "/var/cache/pacman/pkg"
          }
        },
        "encrypted": true,
        "filesystem": {
          "format": "btrfs"
        },
        "wipe": true,
        "mountpoint": "/",
        "size": "100%",
        "start": "513MB",
        "type": "primary"
      }
    ],
    "wipe": true
  },
  "/dev/sda": {
    "partitions": [
      {
        "btrfs": {
          "subvolumes": {
            "@.snapshots": "/.snapshots",
            "@home": "/home"
          }
        },
        "encrypted": true,
        "filesystem": {
          "format": "btrfs"
        },
        "wipe": true,
        "mountpoint": "/",
        "size": "100%",
        "start": "1MB",
        "type": "primary"
      }
    ],
    "wipe": true
  }
}